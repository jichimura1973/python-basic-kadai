      "source": [
        "# クラスHumanを定義する\n",
        "class Human:\n",
        "  # コンストラクタを定義する\n",
        "  def __init__(self, name, age):\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "  # メソッドを定義する\n",
        "  def printinfo(self):\n",
        "    print(f'name:{self.name} age:{self.age}')\n",
        "\n",
        "# インスタンスを作成\n",
        "human = Human(\"侍太郎\", 36)\n",
        "\n",
        "# name,ageを標準出力する\n",
        "human.printinfo()"
      ]
